		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^6.3.6"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	},
	"dependencies": {
		"@edc4it/reveal.js-railroad": "^1.0.5",
		"@fontsource/jetbrains-mono": "^5.2.5",
		"@fontsource/manrope": "^5.2.5",
		"@types/reveal.js": "^5.0.5",
		"add": "^2.0.6",
